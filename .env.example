# Amazing Storage System Environment Variables
# Copy this file to .env and fill in your actual values.
# DO NOT commit the .env file to Git.

# Google Gemini API Key (for chatbot)
ASS_CHATBOT_API_KEY=YOUR_GEMINI_API_KEY_HERE

# Telegram Bot Token (if using the Telegram bot)
ASS_TELEGRAM_BOT_TOKEN=YOUR_TELEGRAM_BOT_TOKEN_HERE

# Optional: Encryption Key (if you were to re-enable encryption)
# ASS_ENCRYPTION_KEY=YOUR_SECRET_ENCRYPTION_KEY

# Credential Filenames (one for each bucket defined in config.json)
# Replace with the actual filenames of your credential files.
ASS_CREDENTIALS_0=google_acc0.json
ASS_CREDENTIALS_1=google_acc1.json
ASS_CREDENTIALS_2=google_acc2.json
ASS_CREDENTIALS_3=dropbox_acc0.txt
ASS_CREDENTIALS_4=dropbox_acc1.txt
ASS_CREDENTIALS_5=dropbox_acc2.txt

# Dropbox App Credentials (if using OAuth 2 flow)
# Get these from your Dropbox App Console (https://www.dropbox.com/developers/apps)
ASS_DROPBOX_APP_KEY=1cuthcog8bad9fy
ASS_DROPBOX_APP_SECRET=2xoqugetiyzlhu8

# The Redirect URI configured in your Dropbox App Console (must match exactly)
ASS_DROPBOX_REDIRECT_URI=http://127.0.0.1:5000/dropbox_callback